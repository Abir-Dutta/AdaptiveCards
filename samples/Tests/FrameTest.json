{    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",    "type": "AdaptiveCard",    "version": "1.0",    "title": "This is the title",    "subtitle": "This is the subtitle",    "heroImage": "https://picsum.photos/300/200?image=900",    "socialImages": {        "heroImage": "https://picsum.photos/300/200?image=700",        "profileImage": "https://picsum.photos/300/200?image=200"    },    "data": {        "data1": "abcdefg",        "data2": "hijklmnop",        "data3": [ "Element One", "Element Two", "Element Three" ],        "data4": {            "insideArray": [ "Element One", "Element Two", "Element Three" ]        },        "dataIndex": 2    },    "body": [        {            "type": "TextBlock",            "text": "This is the body of this card",            "wrap": true        },        {            "type": "TextBlock",            "text": "Here is data1, data2: {{data1}}, {{data2}}.",            "wrap": true        },        {            "type": "TextBlock",            "text": "Here is data4.insideArray[1]: {{data4.insideArray[1]}}.",            "wrap": true        },        {            "type": "TextBlock",            "text": "Here is data4.insideArray[dataIndex]: {{data4.insideArray[dataIndex]}}.",            "wrap": true        }    ],    "actions" :    [        {            "type": "Action.Submit",            "title": "These"        },        {            "type": "Action.Submit",            "title": "are"        },        {            "type": "Action.Submit",            "title": "actions"        }    ]}