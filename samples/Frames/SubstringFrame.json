{
	"$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
	"type": "AdaptiveView",
	"version": "1.0",
    "body": [
        {
            "type": "TextBlock",
            "text": "**Title:**: {{title}}, **Subtitle:** {{subtitle}}.",
            "wrap": true
        },
        {
            "type": "Container",
            "items": "{{body}}"
        },
        {
            "type": "ActionSet",
            "actions": "{{actions}}"
        }
    ]
}